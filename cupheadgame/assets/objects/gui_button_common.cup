b5.export = function(world, scene, data, game) {
	var myexports = {
		createButton: function(n, w, h) {
			var button = new b5.Actor();
			button.ignore_atlas_size = true;
			button.w = w;
			button.h = h;
			button.touchable = true;
			button.name = n;
			button.value = 0;
			return button;
		}
	};
	return myexports;
};